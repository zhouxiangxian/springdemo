		http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-4.1.xsd
		http://www.springframework.org/schema/context 
		http://www.springframework.org/schema/context/spring-context-4.1.xsd">
	<context:annotation-config />
	<bean id="msg" class="cn.mldn.service.impl.MessageServiceImpl" />
	<bean id="dept" class="cn.mldn.vo.Dept" p:deptno="10" p:dname="开发一部" p:loc="深圳"/>
	<bean id="emp" class="cn.mldn.vo.Emp" p:ename="xiaoxian" p:sal="230.0" p:dept-ref="dept" p:empno="100"></bean>
</beans> 